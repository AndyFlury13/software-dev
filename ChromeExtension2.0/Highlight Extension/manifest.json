{
  "name": "Highlight Extension",
  "version": "1.2",
  "description": "Highlights specific phrases in various articles.",
  "manifest_version": 2,
  "content_scripts":
    [{
			"matches": ["*://*/*"],
			"js": ["content_script.js"],
			"run_at": "document_end"
    }],
  "web_accessible_resources": ["*.json"]
}
